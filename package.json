{
    "name": "grabbyright",
    "displayName": "grabbyright",
    "description": "An extension to run grabbyright",
    "version": "0.0.2",
    "publisher": "bongnv",
    "repository": "https://github.com/bongnv/vscode-grabbyright",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:go"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.grabbyright",
                "title": "Grabbyright"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "grabbyright configuration",
            "properties": {
                "grabbyright.onSave": {
                    "type": "boolean",
                    "default": true,
                    "description": "Run grabbyright when saving documents."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}